{
  "name": "intowebdevelopment/workflow-bundle",
  "type": "symfony-bundle",
  "description": "An easy way to define your workfows progmatically and makes defining workflows easier than ever. Our bundle supports actions per step and automatic step transitioning.",
  "keywords": ["Symfony2", "Workflow", "WorkflowBundle", "WorkflowEvent", "Event", "EventBased"],
  "license": "MIT",
  "authors": [
    {
      "name": "IntoWebDevelopment",
      "email": "info@intowebdevelopment.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "IntoWebDevelopment\\WorkflowBundle\\": "src"
    }
  },
  "require": {
    "php": ">=5.3.3",
    "symfony/dependency-injection": "~2.7",
    "symfony/validator": "~2.7",
    "symfony/event-dispatcher": "~2.7",
    "symfony/security-core": "~2.7",
    "doctrine/orm": ">=2.2"
  }
}
